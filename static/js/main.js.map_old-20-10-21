ed = false;\r\n        \r\n        Tabs.setDefaults();\r\n        Tabs.addEvents();\r\n        Tabs.checkHash();\r\n    },\r\n    \r\n    setDefaults: () => {\r\n        \r\n        Tabs.content.each(function () {\r\n            $(this).find('.tabs-content-item').first().addClass('active');\r\n        })\r\n    },\r\n    \r\n    addEvents: () => {\r\n        Tabs.head.parents('.tabs').css('height', Tabs.head.get(0).scrollHeight || '');\r\n        $(window).on('load', function(){\r\n            Tabs.head.parents('.tabs').css('height', Tabs.head.get(0).scrollHeight || '');\r\n            Tabs.obj.find('.tabs-head').addClass('active')\r\n        });\r\n        \r\n        Tabs.head.find('.tabs-head-item').click(function (e) {\r\n            e.preventDefault();\r\n            if ($(window).scrollTop() !== Tabs.content.offset().top && !Tabs.hashed)\r\n                Tabs.animateScroll();\r\n            if ($(this).hasClass('active'))\r\n                return;\r\n            \r\n            Tabs.head.find('.active').removeClass('active');\r\n            $(this).addClass('active');\r\n            var url = URL.full.substring(0, URL.full.indexOf(URL.hash) == -1 ? URL.full.length : URL.full.indexOf(URL.hash));\r\n            Tabs.content.find('.active').removeClass('active');\r\n            Tabs.content.find('[data-tab=\"' + $(this).data('tab') + '\"]').addClass('active').find('.slick-slider').slick('refresh');\r\n            window.history.pushState(null, null , url + '#' + $(this).data('tab'));\r\n        });\r\n        \r\n        window.Scroll = $(window).scrollTop();\r\n        window.headOffset = Tabs.head.parents('.tabs').next().offset().top - (Tabs.head.parents('.tabs').innerHeight() * 5 / 6);\r\n        \r\n        Tabs.checkScroll();\r\n\r\n        $(window).on('orientationchange', function(){\r\n            if (Tabs.hashed)\r\n                return;\r\n            $('html, body').animate({\r\n                scrollTop: 0\r\n            }, 0);\r\n\r\n           